{
  "name": "data-box",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp _headers dist/ && cp _redirects dist/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:docker": "docker compose -f docker-compose.playwright.yml up --build --abort-on-container-exit",
    "test:e2e:docker:clean": "docker compose -f docker-compose.playwright.yml down -v",
    "knip": "knip",
    "healthcheck": "pnpm lint && pnpm knip && tsc -b --noEmit && pnpm format && pnpm test:unit run",
    "update-deps": "npx npm-check -u",
    "preview": "vite preview",
    "postinstall": "cp node_modules/pdfjs-dist/build/pdf.worker.mjs public/pdf.worker.mjs",
    "deploy": "pnpm build && wrangler pages deploy dist"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.546.0",
    "luxon": "^3.7.2",
    "nanoid": "^5.1.6",
    "oidc-client-ts": "^3.3.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-oidc-context": "^3.3.0",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.9.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-barrel-files": "^3.0.1",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.6.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "fake-indexeddb": "^6.2.4",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.7",
    "jsdom": "^27.0.1",
    "knip": "^5.66.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "engines": {
    "node": ">=22.21.0",
    "pnpm": ">=10.0.0"
  },
  "volta": {
    "node": "22.21.0",
    "pnpm": "10.18.3"
  }
}
